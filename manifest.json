{
    "manifest_version": 3,
    "name": "VCH MySchedule Exporter",
    "description": "VCH MySchedule Exporter is a Google Chrome Extension that exports your work shifts from Vancouver Coastal Health (VCH) MySchedule to a iCalendar (.ics) file. ",
    "version": "1.0",
    "icons": {
        "16": "icons/icon_16.png",
        "32": "icons/icon_32.png",
        "48": "icons/icon_48.png",
        "128": "icons/icon_128.png"
    },
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [
        {
            "matches": [
                "https://myschedule.vch.ca/employee/sched/readonly/employee*"
            ],
            "all_frames": true,
            "js": [
                "jquery-3.7.1.min.js",
                "moment-with-locales.min.js",
                "export.js"
            ]
        }
    ],
    "host_permissions": [
        "https://myschedule.vch.ca/employee/sched/readonly/employee*"
    ]
}
